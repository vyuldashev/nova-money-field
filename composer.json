{
    "name": "workup/nova-money-field",
    "description": "A Laravel Nova field for Money.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "laravel/nova": "^4.0",
        "moneyphp/money": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Workup\\Nova\\MoneyField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Workup\\Nova\\MoneyField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
