{
  "name": "vyuldashev/nova-money-field",
  "description": "A Laravel Nova field for Money.",
  "keywords": [
    "laravel",
    "nova",
    "nova-4"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "laravel/nova": "^4.0",
    "nova-kit/nova-packages-tool": "^1.0",
    "moneyphp/money": "^3.1|^4.0"
  },
  "autoload": {
    "psr-4": {
      "Vyuldashev\\NovaMoneyField\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Vyuldashev\\NovaMoneyField\\FieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "repositories": [
    { "type": "composer", "url": "https://nova.laravel.com"}
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
